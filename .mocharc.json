{
  "extensions": [
    "js",
    "jsx",
    "ts",
    "tsx"
  ],
  "timeout": 5000,
  "recursive": true,
  "reporter": "spec",
  "require": "./test/utils/setup.js",
  "spec": [
    "test/**/*.spec.ts"
  ],
  "node-option": [
    "experimental-specifier-resolution=node",
    "loader=ts-node/esm"
  ]
}